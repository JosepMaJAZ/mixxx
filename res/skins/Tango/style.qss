    background-color: #666666;
    color: #eeeeee;
    }
  #MixerbarPflButton[displayValue="0"]:hover,
  #MixerbarPflButton[displayValue="1"] {
    background-color: #0f0f0f;
  }
    #MixerbarButton:hover[displayValue="1"]:hover,
    #MixerbarButton:hover[displayValue="2"]:hover {
      border: 1px solid #eeeeee;
    }

#MixerbarTalkover[displayValue="0"],
#MixerbarMute[displayValue="1"] {
  background-color: #333333;
  border: 1px solid #0f0f0f;
}
#MixerbarTalkover[displayValue="1"],
#MixerbarMute[displayValue="0"] {
  background-color: #ff8f00;
