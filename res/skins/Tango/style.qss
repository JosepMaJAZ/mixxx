  }
  #BeatgridFaster {
    image: url(skin:/buttons/btn_beats_faster.svg) no-repeat center center;
  }
  #BeatgridEarlier {
    image: url(skin:/buttons/btn_beats_earlier.svg) no-repeat center center;
  }
  #BeatgridLater {
    image: url(skin:/buttons/btn_beats_later.svg) no-repeat center center;
  }



/*################################################################
 ######## Mixer Bar #############################################
##############################################################*/

#Mixerbar {
  qproperty-layoutAlignment: 'AlignLeft | AlignTop';
  margin: 0px;
  padding: 0px;
  border: 0px;
  qproperty-layoutSpacing: 0px;
}

#MixerbarSection {
  qproperty-layoutAlignment: 'AlignLeft | AlignVCenter';
  margin: 3px 5px 0px 0px;
  background-color: #333333;
}

#Mixerbar_BelowLibrary[displayValue="0"] {
  background: transparent url(skin:/buttons/btn_arrow_downleft.svg) no-repeat center top;
  }
  #Mixerbar_BelowLibrary[displayValue="0"]:hover {
    background: transparent url(skin:/buttons/btn_arrow_downleft_hover.svg) no-repeat center top;
  }
  #Mixerbar_BelowLibrary[displayValue="1"] {
    background: transparent url(skin:/buttons/btn_arrow_upright.svg) no-repeat center top;
  }
  #Mixerbar_BelowLibrary[displayValue="1"]:hover {
    background: transparent url(skin:/buttons/btn_arrow_upright_hover.svg) no-repeat center top;
  }

#MixerbarToggler[displayValue="0"] {
  background: transparent url(skin:/buttons/btn_arrow_right.svg) no-repeat center top;
  }
  #MixerbarToggler[displayValue="0"]:hover {
    background: transparent url(skin:/buttons/btn_arrow_right_hover.svg) no-repeat center top;
  }
  #MixerbarToggler[displayValue="1"] {
    background: transparent url(skin:/buttons/btn_arrow_left.svg) no-repeat center top;
  }
  #MixerbarToggler[displayValue="1"]:hover {
    background: transparent url(skin:/buttons/btn_arrow_left_hover.svg) no-repeat center top;
  }

#Mixerbar_MasterSectionLabel {
  image: url(skin:/buttons/btn_master.svg) no-repeat center center;
}
#Mixerbar_BoothSectionLabel {
  image: url(skin:/buttons/btn_booth.svg) no-repeat center center;
}
#Mixerbar_HeadSectionLabel {
  image: url(skin:/buttons/btn_head.svg) no-repeat center center;
}
#Mixerbar_MicSectionLabel {
  image: url(skin:/buttons/btn_mic.svg) no-repeat center center;
}
#Mixerbar_AuxSectionLabel {
  image: url(skin:/buttons/btn_aux.svg) no-repeat center center;
}
#Mixerbar_MicAuxLabel {
  /* not apllied
  border-left: 1px solid #453117; */
  font-size: 22px/22px;
  font-weight: bold;
  text-align: center;
}

#MixerbarButton,
#MixerbarPflButton,
#MixerbarXfaderButton {
  font-weight: bold;
  border: 1px solid #0f0f0f;
  }
